<%= render "errors"%>


<%= form_with model: @article, data: { turbo: false }  do |f|%>
    <div>
        <%= f.label :title%> <br>
        <%= f.text_field :title%>
    </div>

    <div>
        <%= f.label :description%> <br>
        <%= f.text_area :description%>
    </div>

    <%= f.submit%>
<% end %>